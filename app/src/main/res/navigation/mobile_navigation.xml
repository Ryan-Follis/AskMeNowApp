<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_profile_hub">

    <fragment
        android:id="@+id/navigation_dms"
        android:name="com.example.askmenow.ui.dms.DMsFragment"
        android:label="DMs"
        tools:layout="@layout/fragment_dms" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.askmenow.ui.notifications.NotificationsFragment"
        android:label="Notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_profile_hub"
        android:name="com.example.askmenow.ui.profile_hub.ProfileHubFragment"
        android:label="Profile Hub"
        tools:layout="@layout/fragment_profile_hub" />

    <activity
        android:id="@+id/navigation_map"
        android:name="com.example.askmenow.activities.MapsActivity"
        android:label="Map"
        tools:layout="@layout/activity_maps"/>

    <fragment
        android:id="@+id/navigation_personal_profile"
        android:name="com.example.askmenow.ui.personal_profile.PersonalProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_personal_profile" />
</navigation>